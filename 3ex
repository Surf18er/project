def conv(n): 
    ls = ['', 'а', 'ов']
    n = n % 100
    if n == 1:
        s=ls[0] 
    else:
        i = n % 10
        if i == 1:
            s = ls[0] 
        elif i in [2,3,4]:
            s = ls[1] 
        else:
            s = ls[2] 
    return s 

for i in range(1,101):
    print('{} процент{}'.format(i, conv(i)))
  

    
