ls = []
for i in range(1,1001,2):
  odd_cube = i**3
  ls.append(odd_cube)
  print(ls)
  
s_cube = 0
for i in ls:
  i_s = 0 
  for sub_i in str(i):
   i_s += int(sub_i)
print('сумма цифр которых делится нацело на 7 =', i_s)
if i_s % 7 == 0:
  s_cube += i
else:
  s_cube += 0

  s_c = 0
for i in ls:
  i + 17
  n_s = 0
  for u_i in str(i):
    n_s += int(u_i)
print(' Поcле сложения еще и на 17 =', n_s)
if n_s % 7 == 0:
  s_c += i
else:
  s_c += 0
  
